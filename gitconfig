[alias]
  co = checkout
  s = status
  unadd = "reset HEAD"
  info = "remote -v"
  forget = "update-index --assume-unchanged"
  l = "log --stat --summary"
[color]
  diff = auto
  status = auto
  branch = auto
  ui = auto
[core]
  excludesfile = ~/.gitignore
[apply]
  whitespace = nowarn
[github]
  user = !sh -c 'echo $GITHUB_USER'
  token = !sh -c 'echo $GITHUB_TOKEN'
[push]
  default = current
